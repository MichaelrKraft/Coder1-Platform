{
  "name": "coder1-platform",
  "version": "1.0.0",
  "description": "Coder1 - Complete Autonomous Coding Platform with Smart PRD Generator and IDE",
  "main": "electron-main.js",
  "scripts": {
    "start": "node server-simple.js",
    "start-full": "node server.js",
    "start-electron": "./node_modules/electron/dist/Electron.app/Contents/MacOS/Electron .",
    "start-server": "node server.js",
    "dev": "NODE_ENV=development ./node_modules/electron/dist/Electron.app/Contents/MacOS/Electron .",
    "dev-server": "nodemon src/app-simple.js",
    "build": "electron-builder",
    "dist": "electron-builder --mac",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "archiver": "^5.3.1",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "uuid": "^9.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "electron": "^37.2.4",
    "electron-builder": "^24.9.1",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": "18.x"
  },
  "keywords": [
    "ai",
    "coding",
    "autonomous",
    "agent",
    "express",
    "nodejs",
    "coder1"
  ],
  "author": "MichaelrKraft",
  "license": "MIT",
  "build": {
    "appId": "com.michaelkraft.coder1",
    "productName": "Coder1",
    "directories": {
      "output": "dist"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "static/coder1-logo.png"
    },
    "files": [
      "**/*",
      "!**/*.ts",
      "!**/*.map",
      "!node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!node_modules/**/*.d.ts"
    ]
  }
}
